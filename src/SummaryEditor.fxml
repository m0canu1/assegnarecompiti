<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Button?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="SummaryEditorController">
    <children>
        <Label>TASKS</Label>
        <ListView fx:id="taskListView"></ListView>
        <HBox>
            <Button fx:id="addNewTask" text="ADD"></Button>
            <Button fx:id="bindTask" text="BIND"></Button>
            <Button fx:id="removeTask" text="REMOVE"></Button>
        </HBox>
    </children>

</VBox>
